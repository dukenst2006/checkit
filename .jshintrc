{
  "camelcase":true,
  "freeze":   true,
  "indent":   2,
  "latedef":  true,
  "newcap":   true,
  "noempty":  true,
  "nonbsp":   true,
  "quotmark": "single",
  "unused":   true,
  "trailing": true,
  "maxparams":8,
  "boss":     true,
  "debug":    true,
  "eqnull":   true,
  "expr":     true,
  "immed":    true,
  "-W058":    true, // Missing '()' invoking a constructor
  "laxbreak": true,

  //"noempty":  false,
  //"asi":      true,
  "unused":   false
  //"-W027":    false // lax Unreachable after 'return'
}
