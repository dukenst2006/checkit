{
  "rules": {
    "users": {
      "$user_id": {
        ".write": "!data.exists() || (auth !== null && $user_id === auth.uid)",
        ".read": "auth !== null && $user_id === auth.uid"
      }
    },
    "checks": {
      "$user_id": {
        ".write": "auth.uid === $user_id",
        ".read": "auth.uid === $user_id",
        "$check_id": {
          ".write": true,
          ".read": true
        }
      }
    },
    "queue": {
      ".write": true,
      ".read": true
    }
  }
}
